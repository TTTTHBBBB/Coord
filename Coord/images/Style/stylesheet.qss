}


QTabWidget::pane {
    border: none;
    background: rgb(0,0,20);
}


QTabBar::tab {
    background: rgb(44, 45, 48);
    color: white;
    border: none;
}

QTabBar::tab:selected {
    background: rgb(58, 59, 62);
    color: white;
    border: none;
}




